extends layout

block content
  h1= title
  p Welcome to #{title}. My self shaik abdul khayyam sahib
  - var image_names = ["blue.jpg","black.jpg","green.jpg","white.jpg","yellow.jpg"]
  - for(let i=0; i<3; i++){
    - var j=Math.floor(Math.random()*5);
    - var img_j=image_names[j];
    image(height="300" width="300" src=`\\images\\${img_j}` alt=`${img_j}` )
  - }